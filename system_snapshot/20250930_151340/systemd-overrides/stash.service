cription=Stash - Porn Slinging Service

Wants=network.target
After=syslog.target network-online.target

[Service]
Type=simple
ExecStart=/home/startup/stash-linux --port 9999 -c /home/startup/.stash/config.yml
Restart=on-failure
RestartSec=10
KillMode=process
StandardOutput=syslog
StandardError=syslog

[Install]
WantedBy=multi-user.target
